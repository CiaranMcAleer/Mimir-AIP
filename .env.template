# Mimir-AIP Environment Configuration Template
# Copy this file to .env and fill in your values

# OpenRouter Integration
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Azure OpenAI Integration
AZURE_OPENAI_API_KEY=your_azure_openai_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/

# Moondream AI Integration
MOONDREAM_API_KEY=your_moondream_key_here

# Database Configuration (if using database plugins)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=mimir
DB_USER=mimir_user
DB_PASSWORD=your_password_here

# General Settings
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR
OUTPUT_DIR=output  # Directory for generated files
CACHE_DIR=cache   # Directory for caching responses